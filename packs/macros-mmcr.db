ere`,\n        `soundfile 3 here`\n    ];\n    let x = number;\n// match the interval to be the same as above in SpellAnimation\n    let interval = 750;\n    for (var i = 0; i < x; i++) {\n        setTimeout(function () {\n            let random = Math.floor(Math.random() * (soundFile.length));\n            AudioHelper.play({src: soundHome + soundFile[random], volume: 0.3, autoplay: true, loop: false}, true);\n        }, i * interval);\n    }\n}\n// then just define the number of times it will play\nmm_audio(3);\n*/\n\n// Add some Token Magic FX\n/*\nlet params =\n[{\n    filterType: \"wave\",\n    filterId: \"myShockwave\",\n    time: 0,\n    strength: 0.03,\n    frequency: 15,\n    maxIntensity: 4.0,\n    minIntensity: 0.5,\n    padding:25,\n    animated :\n    {\n      time : \n      { \n        active: true, \n        speed: 0.0180,\n        animType: \"move\",\n       }\n    }\n},\n{\n    filterType: \"xfire\",\n    filterId: \"myColdXFire\",\n    time: 0,\n// Change color here in hex format\n    color: 0x04CB13,\n    blend: 1,\n    amplitude: 1,\n    dispersion: 0,\n    chromatic: false,\n    scaleX: 1,\n    scaleY: 1,\n    inlay: false,\n    animated :\n    {\n      time : \n      { \n        active: true, \n        speed: -0.0015, \n        animType: \"move\" \n      }\n    }\n}];\nawait wait(250);\nTokenMagic.addUpdateFiltersOnSelected(params);\n*/\n}\n}\nCast ()\n// Activate this to auto delete TMFX Filters\n/*\nasync function ByeBye() {\nawait wait (2500);\nTokenMagic.deleteFiltersOnSelected();\n}\nByeBye ()\n*/","author":"ccsO3NtPJs2xE1RS","img":"modules/jb2a_patreon/Library/Generic/Explosion/Explosion_01_Green_Thumb.webp","actorIds":[]}
